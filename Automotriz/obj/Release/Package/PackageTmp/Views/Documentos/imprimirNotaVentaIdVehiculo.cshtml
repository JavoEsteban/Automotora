@*<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.js" integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU=" crossorigin="anonymous"></script>*@
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.js" integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU=" crossorigin="anonymous"></script>
<script>

    $(document).ready(function () {

        function formateaNumero(num) {
            var numFormateado = new Intl.NumberFormat("de-DE").format(num);
            return numFormateado;
        }

        let precioVenta = $('#txt_PrecioVenta').text();
        precioVenta = formateaNumero(precioVenta);

        let transferencia = $('#txt_Transferencia').text();
        transferencia = formateaNumero(transferencia);

        let totalSinDescuento = $('#Total_SinDescuentos').text();
        totalSinDescuento = formateaNumero(totalSinDescuento);

        let totalDescuentos = $('#txt_totalDescuento').text();
        totalDescuentos = formateaNumero(totalDescuentos);

        let totalPagar = $('#txt_totalPagar').text();
        totalPagar = formateaNumero(totalPagar);

        let totalVehiculosPP = $('#txt_TotalVehiculoPP').text();
        totalVehiculosPP = formateaNumero(totalVehiculosPP);

        let totalDescontado = $('#txt_TotalDescuentos').text();
        totalDescontado = formateaNumero(totalDescontado);

        let totalPagado = $('#txt_TotalPagado').text();
        totalPagado = formateaNumero(totalPagado);

        let PrecioCompra = $('.txt_precioCompra');


        $(PrecioCompra).each(function (index, item) {
            let montoFormateado = formateaNumero($(item).text());
            $(item).text(montoFormateado);
        });

        let montoDescuento = $('.montoDescuento').text();
        montoDescuento = formateaNumero(montoDescuento);
        let montos = $('.txt_montoPago');

        $(montos).each(function (index, item) {
            let montoFormateado = formateaNumero($(item).text());
            $(item).text(montoFormateado);
        });




        $('#txt_PrecioVenta').text(precioVenta);
        $('#txt_Transferencia').text(transferencia);
        $('#Total_SinDescuentos').text(totalSinDescuento);
        $('#txt_totalDescuento').text(totalDescuentos);
        $('#txt_totalPagar').text(totalPagar);
        $('#txt_TotalVehiculoPP').text(totalVehiculosPP);
        $('#txt_TotalDescuentos').text(totalDescontado);
        $('#txt_TotalPagado').text(totalPagado);
        $('.montoDescuento').text(montoDescuento);


    });


    function imprimir() {

        $('.btnImprimir').css("display", "none");
        window.print();
        $('.btnImprimir').css("display", "block");

    }



</script>
