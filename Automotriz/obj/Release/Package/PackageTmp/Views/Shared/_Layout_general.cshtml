    <script type="text/javascript">
        $(document).ready(function () {
            //recarga la pagina si se llega mediante back button
            if (performance.navigation.type == 2) {
                gfProceso();
                location.reload(true);
            }
        });

        function subiraFormulario(idDiv) {
            let elemento = document.getElementById(idDiv);
            elemento.scrollIntoView();
        }

        function gfProceso() {
            if (document.getElementById("divProceso").style.visibility == "visible")
                document.getElementById("divProceso").style.visibility = "hidden";
            else
                document.getElementById("divProceso").style.visibility = "visible";
        }

        function scrollToDiv(idDiv) {
            //var divTopDistance = $('#' + idDiv).position().top;
            //var divDestino = divTopDistance * 1.085 + 70;
            let elemento = document.getElementById();
            elemento.scrollIntoView();
            //$('.sidebar .sidebar-wrapper, .main-panel').animate({ scrollTop: divDestino });
        }

        function formateaNumero(num) {
            var numFormateado = new Intl.NumberFormat("de-DE").format(num);
            return numFormateado;
        }

        function limpiaNumero(numString) {
            var numeroLimpio = numString.replace(/[$.,&nbsp;]/g, '');
            return parseInt(numeroLimpio);
        }

        function Nuevo() {

            location.reload();


        }

        //Solo permite introducir numeros.
        function soloNumeros(e) {
            var key = window.event ? e.which : e.keyCode;
            if (key < 48 || key > 57) {
                e.preventDefault();
            }
        }

        function MaysPrimera(string){
            return string.charAt(0).toUpperCase() + string.slice(1);
        }

    </script>
