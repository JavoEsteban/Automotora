    function diasStock(fechaIngreso) {
        let today = new Date();
        let fechaInicial = fechaIngreso;
        let inicio = new Date(fechaInicial);

        let diasdif = today.getTime() - inicio.getTime();
        let contdias = Math.round(diasdif / (1000 * 60 * 60 * 24));
        return contdias;

    }









